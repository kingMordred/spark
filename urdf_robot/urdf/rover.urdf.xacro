<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="urdf_robot">
  


  <link name="base_footprint">
    
  </link>

  <material name="grey">
    <color rgba="0.8 0.8 0.8 0.8"/>
  </material>
  <material name="black">
    <color rgba="0.1 0.1 0.1 1"/>
  </material>

  <link name="base_link">
  <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="1"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <geometry>
        <box size="0.5 0.43 0.02"/>
      </geometry>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <box size="0.5 0.43 0.02"/>
      </geometry>
    </collision>
  </link>

  <link name="torso">
  <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="1"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <geometry>
        <cylinder length="0.8" radius="0.03"/>
      </geometry>
      <origin rpy="0 0 0" xyz="0 0 0.4"/>
      <material name="grey"/>
    </visual>
    <collision>
      <geometry>
        <cylinder length="0.8" radius="0.03"/>
      </geometry>
    </collision>
  </link>

  <joint name="base_joint" type="fixed">
    <parent link="base_footprint"/>
    <child link="base_link"/>
    <origin xyz="0.0 0.0 0.07" rpy="0.0 0.0 0.0"/>
  </joint>
  

  <joint name="base_to_right_leg" type="fixed">
    <parent link="base_link"/>
    <child link="torso"/>
  </joint>

  <!--front right wheel-->
  <link name="wheel_frontr_link">
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="0.5"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <cylinder radius="0.07" length="0.03"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <cylinder radius="0.07" length="0.03"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_frontr_wheel" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_frontr_link"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" velocity="50.0" effort="1000.0"/>
    <origin xyz="0.13 0.215 0" rpy="1.57 0 0"/>
  </joint>
  

  <!--front left wheel-->
  <xacro:property name="side" value="front_left"/>

  <link name="wheel_${side}_link">
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="0.5"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <cylinder radius="0.07" length="0.03"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <cylinder radius="0.07" length="0.03"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_${side}_frontl" type="continuous">
    <parent link="base_link"/>
    <child link="wheel_${side}_link"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" velocity="50.0" effort="1000.0"/>
    <origin xyz="0.13 -0.215 0" rpy="1.57 0 0"/>
  </joint>

  <!--rear wheel-->

  <link name="rear_wheel">
    <inertial>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <mass value="0.2"/>
      <inertia ixx="0.0" ixy="0.0" ixz="0.0" iyy="0.0" iyz="0.0" izz="0.0"/>
    </inertial>
    <visual>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <cylinder radius="0.02" length="0.01"/>
      </geometry>
      <material name="black"/>
    </visual>
    <collision>
      <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
      <geometry>
        <cylinder radius="0.07" length="0.03"/>
      </geometry>
    </collision>
  </link>

  <joint name="joint_wheel_rear" type="continuous">
    <parent link="base_link"/>
    <child link="rear_wheel"/>
    <axis xyz="0 0 1"/>
    <limit lower="-3.14159" upper="3.14159" velocity="50.0" effort="1000.0"/>
    <origin xyz="-0.17 0 -0.03" rpy="1.57 0 0"/>
  </joint>

  
  

</robot>  